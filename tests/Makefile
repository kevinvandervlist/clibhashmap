MKEXEC=chmod +x

CFLAGS=-Wextra -Wall -Werror -g -ggdb

all: build

clean: 
	@$(RM) *.test

prepare_ld_library_path:
	@$(ECHO) export LD_LIBRARY_PATH=../lib/ > $(RUNTEST)
	@$(MKEXEC) $(RUNTEST)

build: prepare_ld_library_path
